    # original Windows flags.
    &quot;left&quot;:   (((1, 0), 1),  # down
               ((1, 0), 0)),  # up
    &quot;right&quot;:  (((3, 0), 1),
               ((3, 0), 0)),
    &quot;middle&quot;: (((2, 0), 1),
               ((2, 0), 0)),

    # We call these &quot;four&quot; and &quot;five&quot; because Windows calls them that.
    &quot;four&quot;: (((&quot;four&quot;, 0), 1),
             ((&quot;four&quot;, 0), 0)),
    &quot;five&quot;: (((&quot;five&quot;, 0), 1),
